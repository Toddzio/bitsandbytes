<%= form_for([@board, @entries]) do |f| %>
  <div class="field">
    <%= f.label :URL %><br>
    <%= f.text_field :url %>
  </div>
  <div class="field">
    <%= f.label :note %><br>
    <%= f.text_area :note %>
  </div>
  <div>
  <%= f.hidden_field :board_id, value: @board.id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>